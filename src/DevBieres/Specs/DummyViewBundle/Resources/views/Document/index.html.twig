{% extends 'DevBieresSpecsDummyViewBundle::layout.html.twig' %}

{% block app_content %}
<h1> 
   {{ 'document.title'|trans }} 
   <div class="pull-right" >
        <a class="btn btn-primary" id="newDocBtn" type="button" href="{{ path("document_new") }}" rel="tooltip" title="{{ "tooltip.add"|trans }}" >
            {{ 'plus'| ico }}
        </a>
   </div>
</h1>
<div class="row" >
   <div class="col-md12" >
        {{ 
           include('DevBieresSpecsDummyViewBundle:Document:list.html.twig',
                         {
                              cols : cols
                         }
            )
        }}
   </div>
</div>
{% endblock %}
